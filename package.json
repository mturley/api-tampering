  "bugs": {
    "url": "https://github.com/mturley/api-tampering/issues"
  },
  "homepage": "https://github.com/mturley/api-tampering#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
