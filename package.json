{
  "name": "api-tampering",
  "version": "1.0.14",
  "description": "API team too busy? Can't test your UI because you just need that one field changed? Stop waiting, start tampering.",
  "main": "index.js",
  "scripts": {
    "install": "node bin/post-install.js",
    "clean": "rimraf ./dist",
    "clean:build": "npm run clean; npm run build",
    "build": "./node_modules/.bin/babel  ./src --experimental --source-maps-inline -d ./dist",
    "build:main": "npm run clean:build; npm run main",
    "main": "node dist/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "api-tampering": "dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mturley/api-tampering.git"
  },
  "author": "",
